<% if(heading != "" && heading != "undefined" || img_src != "" && img_src != "undefined"  || price != "" && price != "undefined"  || currency != "" && currency != "undefined"){ %>
    <div class="pd_ov">
        <div class="ov_img">
            <img src="<%= img_src %>" alt="Please wait..." class="over_img">
        </div>
        <div class="ov_title">
            <h4 class="ov_title_h4"><%- heading %></h4>
        </div>
        <div class="ov_price">
            <h5 class="ov_price_h5"><span class="curr"><%- currency %></span><span class="am">&nbsp;<%- price %></span><span class="disc"> 10% Discount</span> </h5>
        </div>
        <div class="ov_quant">
            <label for="quantity">QTY</label>
            <input type="number" id="ov_quant" placeholder="1">
            <button class="o_prc dis" id="rzp-button1">Pay Now</button>
        </div>
    </div>
<% } %>
<% if(heading == "" && heading == "undefined" || img_src == "" && img_src == "undefined"  || price == "" && price == "undefined"  || currency == "" && currency == "undefined"){ %>
    <div class="ov_title">
        <h4 class="ov_title_h4">There is no Product</h4>
    </div>
<% } %>